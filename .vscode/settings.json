{
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/.git/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/.cache/**": true,
    "**/.vercel/**": true,
    "**/.turbo/**": true,
    "**/out/**": true,
    "**/coverage/**": true,
    "**/public/**": true,
    "**/ads/**": true,
    "**/pnpm-lock.yaml": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/build": true,
    "**/dist": true,
    "**/.git": true,
    "**/coverage": true,
    "**/public": true,
    "**/ads": true,
    "**/pnpm-lock.yaml": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.next": true
  },
  "antigravity.languageServer.indexExcludedDirectories": [
    "node_modules",
    ".next",
    ".git",
    "build",
    "dist",
    ".cache",
    ".vercel",
    ".turbo",
    "out",
    "coverage",
    "public",
    "ads",
    "pnpm-lock.yaml",
    "**/*.png",
    "**/*.jpg",
    "**/*.jpeg",
    "**/*.svg",
    "**/*.ico",
    "**/*.mp4"
  ],
  "typescript.tsserver.maxTsServerMemory": 768,
  "typescript.disableAutomaticTypeAcquisition": true
}
